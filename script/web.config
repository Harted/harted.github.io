<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <staticContent>
            <remove fileExtension=".css" />
            <remove fileExtension=".less" />
            <mimeMap fileExtension=".less" mimeType="stylesheet/less" />
            <mimeMap fileExtension=".css" mimeType="text/css" />
        </staticContent>
    </system.webServer>
</configuration>
